@model RapidAuto.MVC.Models.Commandes.Commande

@{
    ViewData["Title"] = "Supprimer";
}

<h1>Supprimer</h1>

<h3>Êtes-vous sûr de voulloir supprimer cette commande?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UtilisateurID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UtilisateurID)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.VehiculeID)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.VehiculeID)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="Supprimer" class="btn btn-danger" /> |
        <a asp-action="Index">Retour à la liste des commandes</a>
    </form>
</div>
